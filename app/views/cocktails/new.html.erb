<a class='btn btn-primary' href="/cocktails">Back</a>
<div class='container'>
  <p>
    <strong>
      <h1>Add a new cocktail</h1>
    </strong>
  </p>
  <div class='container'>
    <%= simple_form_for (@cocktail) do |f| %>
    <%= f.input :name %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.submit 'Enter', class: 'btn btn-primary'%>
    <% end %>
  </div>
</div>

