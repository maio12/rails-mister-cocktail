<a class='btn btn-primary' href="/cocktails">Back</a>
<div class='container'>

<h1>Add new dose to cocktail</h1>
<%= simple_form_for [@cocktail,@dose], url: cocktail_doses_path  do |f| %>
  <%= f.association :ingredient %>
  <%= f.input :description %>
  <%= f.button :submit, class: 'btn btn-primary' %>
<% end %>
</div>
